#include<stdio.h>
#include<conio.h>
int main(){
  int rows,cols,i,j;
  /*To get the number of rows and columns from the user*/
  printf("Enter the no .of rows: ");
  scanf("%d",&rows);
  printf("entre the no. of cols: ");
  scanf("%d",&cols);
  
  /*Appending the values to the matrix*/
  for (i=1;i<=rows;i++){
    for (j=cols;j>cols-i;j--){
      print("%d",j);
        for(j=1;j<=cols-i;j++)
        {
          printf("%d",(rows-1+1));
        }
      printf("\n");
      }
  return 0;
      }
      }
